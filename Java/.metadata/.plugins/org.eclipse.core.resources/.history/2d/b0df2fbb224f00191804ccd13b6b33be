package assigment1.tucn.cs.DAL;

public class CoursBuilder {

	private Cours cours;

	public CoursBuilder() {
		cours = new Cours();
	}

	public CoursBuilder setId(Long id) {
		cours.setIdCours(id);
		return this;
	}

	public CoursBuilder setCoursName(String coursName) {
		cours.setCoursName(coursName)
		return this;
	}

	public CoursBuilder setGroup(String group) {
		cours.setGroup(group);
		return this;
	}

	public CoursBuilder setUserName(String userName) {
		cours.setUserName(userName);
		return this;
	}

	public CoursBuilder setPassword(String password) {
		cours.setPassword(password);
		return this;
	}

	public Student build() {
		return cours;
	}

}
