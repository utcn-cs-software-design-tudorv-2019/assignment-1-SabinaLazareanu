package assigment1.tucn.cs.DAL;

public class UserBuilder {

	private User user;

	public UserBuilder(User user) {
		this.user = user;
	}

	public UserBuilder setId(Long id) {
		user.setIdUser(id);
		return this;
	}

	public UserBuilder setName(String name) {
		user.setName(name);
		return this;
	}
	
	public UserBuilder setAddress(String address) {
		user.setAddress(address);
		return this;
	}

	public UserBuilder setPNC(String pnc) {
		user.setPNC(pnc);
		return this
	}
	
	public UserBuilder setINC(String inc) {
		user.setICN(inc);
		return this
	}
	public StudentBuilder setLastName(String lastName) {
		user.setLast_name(lastName);
		return this;
	}

	public StudentBuilder setUserName(String userName) {
		user.setUser_name(userName);
		return this;
	}

	public StudentBuilder setPassword(String password) {
		user.setPassword(password);
		return this;
	}

	public StudentBuilder setGroupId(Long groupId) {
		user.setId_group(groupId);
		return this;
	}

	public Student build() {
		return user;
	}
}
